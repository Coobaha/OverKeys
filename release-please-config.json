{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"packages": {
		".": {
			"package-name": "overkeys",
			"changelog-path": "CHANGELOG.md",
			"release-type": "dart",
			"always-update": true,
			"include-component-in-tag": false,
			"include-v-in-tag": true,
			"bump-minor-pre-major": true,
			"bump-patch-for-minor-pre-major": true,
			"draft": true,
			"changelog-sections": [
				{
					"type": "feat",
					"section": "✨ Features",
					"hidden": false
				},
				{
					"type": "fix",
					"section": "🐛 Bug Fixes",
					"hidden": false
				},
				{
					"type": "perf",
					"section": "⚡ Performance Improvements",
					"hidden": false
				},
				{
					"type": "chore",
					"section": "🧹 Miscellaneous Chores",
					"hidden": false
				},
				{
					"type": "docs",
					"section": "📖 Documentation",
					"hidden": false
				},
				{
					"type": "ci",
					"section": "🚀 CI/CD Pipeline",
					"hidden": false
				},
				{
					"type": "tests",
					"section": "🔧 Testing",
					"hidden": false
				},
				{
					"type": "style",
					"section": "💅 Code Style",
					"hidden": false
				},
				{
					"type": "refactor",
					"section": "🔨 Refactoring",
					"hidden": false
				},
				{
					"type": "revert",
					"section": "⏪ Reverts",
					"hidden": false
				}
			],
			"pull-request-title-pattern": "chore: release v${version}",
			"group-pull-request-title-pattern": "chore: release v${version}",
			"pull-request-header": "🤖 chore: release v${version}"
		}
	}
}
