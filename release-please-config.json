{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"packages": {
		".": {
			"package-name": "overkeys",
			"initial-version": "0.2.6",
			"changelog-path": "CHANGELOG.md",
			"release-type": "dart",
			"always-update": true,
			"include-component-in-tag": false,
			"include-v-in-tag": true,
			"bump-minor-pre-major": true,
			"bump-patch-for-minor-pre-major": true,
			"draft": true,
			"changelog-sections": [
				{
					"type": "feat",
					"section": "✨ Features",
					"hidden": false
				},
				{
					"type": "fix",
					"section": "🐛 Bug Fixes",
					"hidden": false
				},
				{
					"type": "perf",
					"section": "🚀 Performance Improvements",
					"hidden": false
				},
				{
					"type": "chore",
					"section": "🧹 Miscellaneous Chores",
					"hidden": true
				},
				{
					"type": "docs",
					"section": "📚 Documentation",
					"hidden": true
				},
				{
					"type": "ci",
					"section": "⚙️ CI/CD Pipeline",
					"hidden": true
				},
				{
					"type": "tests",
					"section": "🔧 Testing",
					"hidden": true
				},
				{
					"type": "style",
					"section": "💅 Code Style",
					"hidden": true
				},
				{
					"type": "refactor",
					"section": "🔨 Refactoring",
					"hidden": true
				},
				{
					"type": "revert",
					"section": "⏪ Reverts",
					"hidden": true
				}
			],
			"pull-request-title-pattern": "chore: release v${version}",
			"group-pull-request-title-pattern": "chore: release v${version}",
			"pull-request-header": "🤖 New release prepared",
			"extra-files": [
				{
					"path": "scripts/compile_exe-inno.iss",
					"type": "generic"
				}
			]
		}
	}
}
